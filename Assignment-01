Q.8)
importar java.util.Scanner;  
public class interestS{
 public static void main (String ar []){
    Scanner sc = new Scanner (System.in); 
System.out.print ("Enter the principal amount:");
 double pa = sc.nextDouble ();  
System.out.print ("Enter the rate of interest:");  
double ri = sc.nextDouble ();
System.out.print ("Enter the time:");  
double t = sc.nextDouble ();  
  double SI = (pa * ri * t)/100.0;
System.out.println("Simple interest = $"+SI);
  }
}

$javac interestS.java
$java interestS

Q.9)

import java.util.Scanner;
public class Year_Week_Day 
{
    public static void main(String args[])
    {
        int m, year, week, day;
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number of days:");
        int x = sc.nextInt();
        int year = x / 365;
        x = x % 365;
        System.out.println("No. of years:"+year);
        int week = x / 7;
        x = x % 7;
        System.out.println("No. of weeks:"+week);
        int day = x;
        System.out.println("No. of days:"+day);
    }
}

$ javac Year_Week_Day.java
$ java Year_Week_Day

Q.10)
import java.util.Scanner;
public class Fahrenheit
{
	public static void main(String arg[])	
	{
	    double a,c;
             	    Scanner sc=new Scanner(System.in);	   	 
	    System.out.println("Enter  Fahrenheit temperature");
                   a=sc.nextDouble(); 
	    System.out.println("Celsius temperature is = "+celsius(a));		  	  	     
	} 
	static double celsius(double f)
	{	
	return  (f-32)*5/9;
	}
}

$ javac Fahrenheit.java
$ java Fahrenheit

Q.11)
import java.util.Scanner; 
public class Swapping{
 public static void main (String []args){
      Scanner sc = new Scanner (System.in);
           System.out.println ("Enter the Numbers" ); 
             int x = sc.nextInt(); 
             int y = sc.nextInt(); 
           System.out.println ("Number before swap"); 
           System.out.println(x); 
           System.out.println(y); 
              x=x+y; 
              y=x-y; 
              x=x-y; 
       System.out.println ("Number after swap" ); 
       System.out.println(x); 
       System.out.println(y); 
  } 
 }

$ javac Swapping.java
$ java Swapping

Q.12)
import java.util.Scanner;
public class Salary{
     
    public static void main(String abc[])
    {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter basic salary");
		float bsal=sc.nextFloat();
		if(bsal < 10000)
		{
			float HRA=(bsal*10) /100 ;
			float DA=(bsal*90)/100;
		    float GS=bsal+DA+HRA;
			System.out.println("Gross sal for bsal " +bsal + " is " +GS );
		}
		else if(bsal >= 10000)
		{
		    float HRA=2000;
			float DA=(bsal * 90)/100;
		    float GS=bsal+DA+HRA;
			System.out.println("Gross sal for bsal " +bsal + " is " +GS );
		}
		else{
			
			System.out.println("Invalid sal" );
	}
}
}

$ javac Salary.java
$ java Salary

Q.13)(A: if-else)
import java.util.Scanner; 
public class gretestN{ 
public static void main (String []args){ 
Scanner sc = new Scanner (System.in); 
int A = sc.nextInt(); 
int B = sc.nextInt(); 
int C = sc.nextInt();
 if(A>B && A>C){ 
System.out.println(A+" is gretest number");
 }
 if(B>A && B>C){
 System.out.prihtln(B +"is gretest number"); 
}
else{
 System.out.println(C+" is greatest number);
  }
 }
}

$javac gretestN.java
$java gretestN

(B: Ternary Operator)

import java.util.Scanner ;
Public class tarnariOp {

	public static void main(String args[]){
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter the numbers");
	int i = sc.nextInt();
	int j = sc.nextInt();
	int k = sc.nextInt();

	String s =(i > j && i > k)? "i is greater": j>k ? "J is greater" : "K is greater";
System.out.println(s);
	}
}

$ javac tarnariOp.java
$ java tarnariOp


